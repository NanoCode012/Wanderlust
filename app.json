{
    "expo": {
        "name": "Wanderlust",
        "slug": "Wanderlust",
        "description": "React Native for travel",
        "primaryColor": "#CB0C9F",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#CF5D9B"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#CF5D9B"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
